{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": "current"
            },
            "corejs": 3,
            "debug": true,
            "useBuiltIns": "usage"
        }]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime"],
        ["@babel/plugin-proposal-class-properties", {"loose": true}],
        ["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }]
    ]
}
